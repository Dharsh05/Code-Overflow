#include <iostream>  
using namespace std;  
int main()  
{  
int n,r,sum=0;
int temp;    
cout<<"Enter Number";    
cin>>n;    
temp=n;    
while(n>0)    
{    
r=n%10;    
sum=sum+(r*r*r);    
n=n/10;    
}    
if(temp==sum)    
cout<<"Given number is an Armstrong Number."<<endl;    
else    
cout<<"Given number is not Armstrong Number."<<endl;   
return 0;  
}  
